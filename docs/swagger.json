        "/controller": {
            "get": {
                "description": "返回控制器运行状态、策略列表及当前生效策略",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Controller"
                ],
                "summary": "获取控制器状态",
                "responses": {
                    "200": {
                        "description": "成功获取控制器状态",
                        "schema": {
                            "$ref": "#/definitions/server.ControllerResponse"
                        }
                    },
                    "500": {
                        "description": "响应编码失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "put": {
                "description": "动态更新运行状态、控制频率或切换调度策略",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Controller"
                ],
                "summary": "更新控制器配置",
                "parameters": [
                    {
                        "description": "更新配置参数",
                        "name": "config",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/server.UpdateControllerRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "配置更新成功"
                    },
                    "400": {
                        "description": "请求解析失败/策略不存在",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "内部配置更新失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/monitor": {
            "get": {
                "description": "获取系统中存在的所有监控配置列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Monitor"
                ],
                "summary": "获取所有监控配置",
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.Monitor"
                            }
                        }
                    },
                    "500": {
                        "description": "获取数据失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "post": {
                "description": "创建监控请求并部署到Kubernetes集群，包含MDE和KPI组件",
