                    {
                        "description": "监控配置对象",
                        "name": "monitor",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.Monitor"
                        }
                    }
                ],
                "responses": {
                    "201": {
                        "description": "创建成功返回监控对象",
                        "schema": {
                            "$ref": "#/definitions/model.Monitor"
                        }
                    },
                    "400": {
                        "description": "请求解码失败/参数验证失败/Slice不存在",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "关联Slice不存在",
