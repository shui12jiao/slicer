{
    "swagger": "2.0",
    "info": {
        "description": "Slicer API\n基于Kubernetes资源的切片管理系统API\n包括 切片管理 监控管理 性能保证 等功能",
        "title": "Slicer API",
        "contact": {},
        "version": "1.0"
    },
    "host": "localhost:30001",
    "basePath": "/",
    "paths": {
        "/slice": {
            "get": {
                "description": "获取当前系统中的所有切片列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "slice"
                ],
                "summary": "获取所有切片",
                "responses": {
                    "200": {
                        "description": "获取成功，返回切片列表",
                        "schema": {
                            "type": "array",
                            "items": {
                                "$ref": "#/definitions/model.SliceAndAddress"
                            }
                        }
                    },
                    "500": {
                        "description": "服务器内部错误（获取列表失败、响应编码失败）",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "post": {
                "description": "接受一个切片对象，创建一个新的切片，并返回创建的切片对象",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "slice"
                ],
                "summary": "创建切片",
                "parameters": [
                    {
                        "description": "切片对象",
                        "name": "slice",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/model.Slice"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "创建成功，返回切片及其地址",
                        "schema": {
                            "$ref": "#/definitions/model.SliceAndAddress"
                        }
                    },
                    "400": {
                        "description": "请求格式错误或参数非法",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "409": {
                        "description": "切片已存在",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误，如分配IP或部署资源失败",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "/slice/{sliceId}": {
            "get": {
                "description": "根据切片ID获取指定切片的详细信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "slice"
                ],
                "summary": "获取单个切片",
                "parameters": [
                    {
                        "type": "string",
                        "description": "切片ID",
                        "name": "sliceId",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "获取成功",
                        "schema": {
                            "$ref": "#/definitions/model.SliceAndAddress"
                        }
                    },
                    "400": {
                        "description": "缺少sliceId参数",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "切片不存在",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误（获取失败、响应编码失败）",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            },
            "delete": {
                "description": "根据切片ID删除指定的切片资源",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "slice"
                ],
                "summary": "删除切片",
                "parameters": [
                    {
                        "type": "string",
                        "description": "切片ID",
                        "name": "sliceId",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "204": {
                        "description": "删除成功无内容"
                    },
                    "400": {
                        "description": "缺少sliceId参数",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "切片不存在",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "500": {
                        "description": "服务器内部错误（获取/渲染/删除k8s资源失败、释放IP失败、存储删除失败）",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "model.AMBR": {
            "type": "object",
            "properties": {
                "downlink": {
                    "$ref": "#/definitions/model.BitRate"
                },
                "uplink": {
                    "$ref": "#/definitions/model.BitRate"
                }
            }
        },
        "model.ARP": {
            "type": "object",
            "properties": {
                "pre_emption_capability": {
                    "type": "integer"
                },
                "pre_emption_vulnerability": {
                    "type": "integer"
                },
                "priority_level": {
                    "type": "integer"
                }
            }
        },
        "model.BitRate": {
            "type": "object",
            "properties": {
                "unit": {
                    "type": "integer"
                },
                "value": {
                    "type": "integer"
                }
            }
        },
        "model.Flow": {
            "type": "object",
            "properties": {
                "description": {
                    "type": "string"
                },
                "direction": {
                    "type": "integer"
                }
            }
        },
        "model.GBR": {
            "type": "object",
            "properties": {
                "downlink": {
                    "$ref": "#/definitions/model.BitRate"
                },
                "uplink": {
                    "$ref": "#/definitions/model.BitRate"
                }
            }
        },
        "model.MBR": {
            "type": "object",
            "properties": {
                "downlink": {
                    "$ref": "#/definitions/model.BitRate"
                },
                "uplink": {
                    "$ref": "#/definitions/model.BitRate"
                }
            }
        },
        "model.PCCRule": {
            "type": "object",
            "properties": {
                "flow": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.Flow"
                    }
                },
                "qos": {
                    "$ref": "#/definitions/model.QoS"
                }
            }
        },
        "model.QoS": {
            "type": "object",
            "properties": {
                "arp": {
                    "$ref": "#/definitions/model.ARP"
                },
                "gbr": {
                    "$ref": "#/definitions/model.GBR"
                },
                "index": {
                    "type": "integer"
                },
                "mbr": {
                    "$ref": "#/definitions/model.MBR"
                }
            }
        },
        "model.Session": {
            "type": "object",
            "properties": {
                "ambr": {
                    "$ref": "#/definitions/model.AMBR"
                },
                "name": {
                    "type": "string"
                },
                "pcc_rule": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.PCCRule"
                    }
                },
                "qos": {
                    "$ref": "#/definitions/model.QoS"
                },
                "type": {
                    "type": "integer"
                }
            }
        },
        "model.Slice": {
            "type": "object",
            "properties": {
                "default_indicator": {
                    "type": "boolean"
                },
                "id": {
                    "type": "string"
                },
                "sd": {
                    "type": "string"
                },
                "session": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.Session"
                    }
                },
                "sst": {
                    "type": "integer"
                }
            }
        },
        "model.SliceAndAddress": {
            "type": "object",
            "properties": {
                "default_indicator": {
                    "type": "boolean"
                },
                "id": {
                    "type": "string"
                },
                "sd": {
                    "type": "string"
                },
                "session": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/model.Session"
                    }
                },
                "sessionSubnets": {
                    "description": "Subnet是子网",
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "smfn3Addr": {
                    "type": "string"
                },
                "smfn4Addr": {
                    "type": "string"
                },
                "sst": {
                    "type": "integer"
                },
                "upfn3Addr": {
                    "description": "Addr是地址",
                    "type": "string"
                },
                "upfn4Addr": {
                    "type": "string"
                }
            }
        }
    }
}